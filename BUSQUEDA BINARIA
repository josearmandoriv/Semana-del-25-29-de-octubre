#include <conio.h>
#include <stdio.h> 
int binSearch (double v[], int N, double buscado);

int main ()
{
	 double v[]={8, 23, 78, 8, 32, 56};
	 int i;
	 for (i=0;i<6;i++){
	 	printf("%f", v[i]);
	 	binSearch; 
	 }              
	 getch; 
	 return 0;
}  
// ImplementaciÃ³n iterativa
// Uso: binSearch (vector, N, dato)
int binSearch (double v[], int N, double buscado)
{
int izq = 0;
int der = N-1;
int centro = (izq+der)/2;
while ((izq<=der) && (v[centro]!=buscado)) {
if (buscado<v[centro])
der= centro-1;
else
izq= centro+1;
centro= (izq+der)/2;
}
if (izq>der)
return -1;
else
return centro;
}
